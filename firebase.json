{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "site": "livenote-11929",
    "public": "assets",
    "cleanUrls": true,
    "rewrites":[{
      "source":"**",
      "run": {
        "serviceId":"renderer",
        "region": "asia-northeast1"
      }
    }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "functions": { "source": ".output/server" }
}
